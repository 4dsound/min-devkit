<c74object name='banger' category='time'>

	<digest>Bang at random intervals</digest>
	<description>Bang at random intervals.</description>


	<!--METADATA-->

	<metadatalist>
		<metadata name='author'>Cycling '74</metadata>
		<metadata name='tag'>time</metadata>
	</metadatalist>


	<!--MESSAGES-->

	<methodlist>

		<method name='int'>
			<digest>Toggle the state of the timer</digest>
			<description>Toggle the state of the timer.</description>
		</method>

	</methodlist>


	<!--ATTRIBUTES-->

	<attributelist>

		<attribute name='min' get='1' set='1' type='float64' size='1' >
			<digest>Lower-bound of generated random interval</digest>
			<description>Lower-bound of generated random interval</description>
		</attribute>

		<attribute name='max' get='1' set='1' type='float64' size='1' >
			<digest>Upper-bound of generated random interval</digest>
			<description>Upper-bound of generated random interval</description>
		</attribute>

		<attribute name='on' get='1' set='1' type='long' size='1' >
			<digest>Activate the timer</digest>
			<description>Activate the timer.</description>
		</attribute>

	</attributelist>


	<!--RELATED-->

	<seealsolist>
